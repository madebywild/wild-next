{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not safari < 14.1",
      "not iOS < 14.5",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf .next out node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "analyze": "ANALYZE=true npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write '**/*.{json,md,css,js,jsx,ts,tsx}' && npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "13.4.19",
    "next-seo": "6.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.0.7"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.5.14",
    "@madebywild/prettier-config": "0.1.0",
    "@next/bundle-analyzer": "13.4.19",
    "@next/eslint-plugin-next": "13.4.19",
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "babel-plugin-twin": "1.1.0",
    "eslint": "8.48.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "react-is": "18.2.0",
    "rimraf": "5.0.1",
    "tailwindcss": "3.3.3",
    "twin.macro": "3.4.0",
    "typescript": "5.2.2"
  }
}
