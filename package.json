{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf .next && rimraf build && rimraf out && rimraf node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "npm run clear && ANALYZE=true npm run build",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "11.0.0",
    "next-seo": "4.25.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "styled-components": "5.3.0",
    "twin.macro": "2.5.0"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.2.2",
    "@madebywild/prettier-config": "0.1.0",
    "@next/bundle-analyzer": "11.0.0",
    "@svgr/webpack": "5.5.0",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@types/node": "15.12.2",
    "@types/react": "17.0.11",
    "@types/styled-components": "5.1.10",
    "@types/webpack-env": "1.16.0",
    "babel-plugin-twin": "1.0.2",
    "eslint": "7.28.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "tailwindcss": "2.1.4",
    "typescript": "4.3.2"
  }
}
