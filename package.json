{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "clean": "rimraf .next out node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "ANALYZE=true npm run build",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.3.1",
    "next-seo": "5.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.5.10",
    "@madebywild/prettier-config": "0.1.0",
    "@next/bundle-analyzer": "12.3.1",
    "@next/eslint-plugin-next": "12.3.1",
    "@svgr/webpack": "6.3.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/styled-components": "5.1.26",
    "@types/webpack-env": "1.18.0",
    "babel-plugin-twin": "1.0.2",
    "eslint": "8.23.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react-is": "18.2.0",
    "rimraf": "3.0.2",
    "tailwindcss": "2.2.19",
    "twin.macro": "2.8.2",
    "typescript": "4.8.3"
  }
}
