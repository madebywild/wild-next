{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf .next && rimraf build && rimraf out && rimraf node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "npm run clear && ANALYZE=true npm run build",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.2.4",
    "next-seo": "5.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.5.8",
    "@madebywild/prettier-config": "0.1.0",
    "@next/bundle-analyzer": "12.2.4",
    "@next/eslint-plugin-next": "12.2.4",
    "@svgr/webpack": "6.3.1",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@types/node": "18.6.4",
    "@types/react": "18.0.15",
    "@types/styled-components": "5.1.25",
    "@types/webpack-env": "1.17.0",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-twin": "1.0.2",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react-is": "18.2.0",
    "rimraf": "3.0.2",
    "tailwindcss": "2.2.19",
    "typescript": "4.7.4",
    "twin.macro": "2.8.2"
  }
}
