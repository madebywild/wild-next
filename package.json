{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not ie <= 11",
      "not edge <= 18",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf .next out node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "ANALYZE=true npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}' && npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.3.1",
    "next-seo": "6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.10"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.5.11",
    "@madebywild/prettier-config": "0.1.0",
    "@next/bundle-analyzer": "12.3.1",
    "@next/eslint-plugin-next": "12.3.1",
    "@svgr/webpack": "7.0.0",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/styled-components": "5.1.26",
    "@types/webpack-env": "1.18.0",
    "babel-plugin-twin": "1.1.0",
    "eslint": "8.39.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "prettier": "2.8.8",
    "react-is": "18.2.0",
    "rimraf": "5.0.0",
    "tailwindcss": "3.3.1",
    "twin.macro": "3.3.1",
    "typescript": "5.0.4"
  }
}
