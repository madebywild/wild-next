{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf .next && rimraf build && rimraf out && rimraf node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "npm run clear && ANALYZE=true npm run build",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "11.1.2",
    "next-seo": "4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.1"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.4.3",
    "@madebywild/prettier-config": "0.1.0",
    "@next/bundle-analyzer": "11.1.2",
    "@next/eslint-plugin-next": "11.1.2",
    "@svgr/webpack": "5.5.0",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@types/node": "16.10.1",
    "@types/react": "17.0.24",
    "@types/styled-components": "5.1.14",
    "@types/webpack-env": "1.16.2",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-twin": "1.0.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.10.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "react-is": "17.0.2",
    "rimraf": "3.0.2",
    "tailwindcss": "2.2.16",
    "typescript": "4.4.3",
    "twin.macro": "2.7.0"
  }
}
