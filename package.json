{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf .next && rimraf build && rimraf out && rimraf node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "npm run clear && ANALYZE=true npm run build",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.1.4",
    "next-seo": "5.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.5.2",
    "@madebywild/prettier-config": "0.1.0",
    "@next/bundle-analyzer": "12.1.4",
    "@next/eslint-plugin-next": "12.1.4",
    "@svgr/webpack": "6.2.1",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/styled-components": "5.1.24",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-twin": "1.0.2",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.1",
    "react-is": "17.0.2",
    "rimraf": "3.0.2",
    "tailwindcss": "2.2.19",
    "typescript": "4.6.3",
    "twin.macro": "2.8.2"
  }
}
